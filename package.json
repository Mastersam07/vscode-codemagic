{
  "name": "vscode-codemagic",
  "displayName": "Codemagic",
  "description": "YAML schema validation, linting and intellisense/auto-completion for Codemagic",
  "version": "0.1.0",
  "publisher": "Mastersam",
  "homepage": "https://github.com/Mastersam07/vscode-codemagic",
  "repository": {
    "type": "git",
    "url": "https://github.com/Mastersam07/vscode-codemagic"
  },
  "bugs": {
    "url": "https://github.com/Mastersam07/vscode-codemagic/issues"
  },
  "engines": {
    "vscode": "^1.79.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:yaml"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-codemagic.helloWorld",
        "title": "Hello World"
      }
    ],
    "languages": [
      {
        "filenames": [
          "codemagic.yaml"
        ],
        "filenamePatterns": [
          "codemagic.yaml"
        ],
        "id": "yaml"
      }
    ]
  },
  "extensionDependencies": [
    "redhat.vscode-yaml"
  ],
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.79.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "@vscode/test-electron": "^2.3.2"
  }
}